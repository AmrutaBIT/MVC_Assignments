@model List<Day6EFDBDemo.Models.Brand>
@{
    ViewBag.Title = "Brands";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Brands</h2>
<table class="table">
    <tr>
        <th>Brand Id</th>
        <th>Brand Name</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.brandId</td>
            <td>@item.brandName</td>
        </tr>

    }
</table>

